{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug/Game1.exe",
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/mingw64/bin/gdb.exe",
      "setupCommands": [
        { "text": "-enable-pretty-printing", "ignoreFailures": true },
        { "text": "-gdb-set disassembly-flavor intel", "ignoreFailures": true }
      ],
      "preLaunchTask": "debug"
    },
    {
      "name": "debug_Perf",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/debug_perf/Game1.exe",
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/mingw64/bin/gdb.exe",
      "setupCommands": [
        { "text": "-enable-pretty-printing", "ignoreFailures": true },
        { "text": "-gdb-set disassembly-flavor intel", "ignoreFailures": true }
      ],
      "preLaunchTask": "debug_perf"
    }
  ]
}